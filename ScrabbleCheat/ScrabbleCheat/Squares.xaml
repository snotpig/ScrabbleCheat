<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <VisualBrush x:Key="square">
        <VisualBrush.Visual>
            <Canvas Height="50" Width="50" Background="#b0e0cd">
                <Rectangle Canvas.Left="2" Canvas.Top="2" Width="46" Height="46">
                    <Rectangle.Fill>
                        <LinearGradientBrush StartPoint="0, 0" EndPoint="1, 1">
                            <GradientStop Color="#205000" Offset="0"></GradientStop>
                            <GradientStop Color="#306010" Offset="0.5"></GradientStop>
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>
                <Rectangle Canvas.Left="7" Canvas.Top="7" Width="41" Height="41" >
                    <Rectangle.Fill>
                        <LinearGradientBrush  StartPoint="0,0" EndPoint="1,1">
                            <LinearGradientBrush.GradientStops>
                                <GradientStop Offset="0" Color="#206010"/>
                                <GradientStop Offset="1" Color="#308020"/>
                            </LinearGradientBrush.GradientStops>
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>

    <VisualBrush x:Key="dl">
        <VisualBrush.Visual>
            <Canvas Height="50" Width="50" Background="#a7bdd4">
                <Rectangle Fill="#d3dde7" Canvas.Left="1" Canvas.Top="1" Width="48" Height="48"/>
                <Rectangle Canvas.Left="2" Canvas.Top="2" Width="46" Height="46">
                    <Rectangle.Fill>
                        <LinearGradientBrush StartPoint="0, 0" EndPoint="1, 1">
                            <GradientStop Color="#87b3d4" Offset="0"></GradientStop>
                            <GradientStop Color="#90bede" Offset="0.5"></GradientStop>
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>
                <Rectangle Canvas.Left="6" Canvas.Top="6" Width="41" Height="41">
                    <Rectangle.Fill>
                        <LinearGradientBrush StartPoint="0, 0" EndPoint="1, 1">
                            <GradientStop Color="#97c5e6" Offset="0"></GradientStop>
                            <GradientStop Color="#9eccfd" Offset="1"></GradientStop>
                        </LinearGradientBrush>
                    </Rectangle.Fill>
                </Rectangle>
                <TextBlock Canvas.Left="12.5" Canvas.Top="7.0" Foreground="Black" 
                           FontSize="20" FontFamily="Corbel" FontWeight="Bold">DL
                    <TextBlock.RenderTransform>
                        <ScaleTransform ScaleY="1.4"/>
                    </TextBlock.RenderTransform>
                </TextBlock>
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>

