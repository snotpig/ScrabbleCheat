    <VisualBrush x:Key="#">
        <VisualBrush.Visual>
    <Canvas Height="50" Width="50">
        <Rectangle Width="50" Height="50" Fill="Transparent"/>
        <Rectangle Canvas.Left="2" Canvas.Top="2" Height="46" Width="46" 
                           RadiusX="4" RadiusY="4">
            <Rectangle.Fill>
                <RadialGradientBrush>
                    <RadialGradientBrush.GradientStops>
                        <GradientStop Offset="0.93" Color="#f0e8b451"/>
                        <GradientStop Offset="0.97" Color="#f0eab23e"/>
                    </RadialGradientBrush.GradientStops>
                </RadialGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle Canvas.Left="3.5" Canvas.Top="2.5" Height="43.5" Width="43" RadiusX="3.5" RadiusY="3.5">
            <Rectangle.Fill>
                <LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
                    <GradientStop Offset="0.0" Color="#d6d1af7b"/>
                    <GradientStop Offset="0.05" Color="#d0ffffff"/>
                    <GradientStop Offset="0.2" Color="#d0d1af7b"/>
                    <GradientStop Offset="0.70" Color="#d0d1af7b"/>
                    <GradientStop Offset="0.95" Color="#d0dfcf9f"/>
                    <GradientStop Offset="0.99" Color="#d6d9b784"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
    </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>

