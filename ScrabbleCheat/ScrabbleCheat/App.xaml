<Application x:Class="ScrabbleCheat.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
                <ResourceDictionary Source="Squares.xaml"/>
                <ResourceDictionary Source="Tiles.xaml"/>
                <ResourceDictionary Source="BlankTiles.xaml"/>
                <ResourceDictionary Source="Rack.xaml"/>
                <ResourceDictionary Source="Arrows.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
